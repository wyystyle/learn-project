<template name="starTemplate">
<view class='star-box'>
<block wx:for="{{stars}}" wx:for-item="star" wx:key="star">
  <image wx:if="{{star == 1}}" src='/images/icon/star-active.png'></image>
  <image wx:else src='/images/icon/collection-none.png'></image>
</block>
<text class='score'>{{score}}</text>
</view>
</template>